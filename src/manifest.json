{
  "manifest_version": 3,
  "name": "MRGARGSIR Tools",
  "version": "1.6",
  "description": "Adds utility bookmarks for HEWP workflow automation",
  "permissions": [
    "bookmarks"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "MRGARGSIR Tools"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
